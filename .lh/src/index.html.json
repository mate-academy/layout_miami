            "content": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Miami</title>\n    <link\n      rel=\"icon\"\n      href=\"./images/favicon.png\"\n      type=\"image/x-icon\"\n    >\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\">\n    <link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@700&family=Space+Grotesk:wght@400;500;700&family=Space+Mono&display=swap\" rel=\"stylesheet\">\n    <link rel=\"stylesheet\" href=\"./styles/main.scss\">\n    <script src=\"scripts/main.js\" defer></script>\n  </head>\n  <body>\n    <header class=\"header\" id=\"header\">\n      <div class=\"nav header__nav\">\n        <a href=\"#\" class=\"nav__item\">\n          <img\n            src=\"./images/logo-transparent.png\" alt=\"NOTHING logo\"\n            class=\"nav__logo\"\n          >\n        </a>\n        <div class=\"nav__icons\">\n          <a class=\"nav__item phone\" href=\"tel:+1 234 555-55-55\">\n            <span class=\"phone__label\">+1 234 555-55-55</span>\n            <img src=\"./images/icon-phone-call-transparent.png\" alt=\"phone_icon\">\n          </a>\n          <a href=\"#menu\" class=\"nav__item burger\">\n            <span class=\"burger__label\">Open menu</span>\n          </a>\n        </div>\n      </div>\n      <h1 class=\"title header__title\">\n        Bring joy back to\n        the everyday\n      </h1>\n    </header>\n\n    <div class=\"menu\" id=\"menu\">\n      <div class=\"nav header__nav\">\n        <a href=\"#\" class=\"nav__item\">\n          <img\n            src=\"./images/logo-transparent.png\" alt=\"NOTHING logo\"\n            class=\"nav__logo\"\n          >\n        </a>\n        <a href=\"#header\" class=\"nav__item\">\n          <img\n            src=\"./images/icon-close.png\"\n            alt=\"icon_close\"\n        